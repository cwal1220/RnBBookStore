<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="join">

	 <insert id="insert" parameterType="dto.MemDTO">
  insert into mems (memNum,memId,memPass,memEmail)
  values (mem_seq.nextval,#{memId},#{memPass},#{memEmail}) 
 </insert>
	
 
	<select id="content" parameterType="int" resultType="dto.MemDTO"> 
         select * from mems where memNum=#{memNum}     
    </select>
	
	 	

	<select id="lista" resultType="dto.MemDTO">
		select * from mems order by
		Event_Num desc

	</select>


	
</mapper>